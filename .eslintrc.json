{
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "one-var-declaration-per-line": "error",
      "quotes": ["error", "single"],
      "no-var": "off",
      "max-len": "warn",
      "guard-for-in": "warn",
      "eqeqeq": "warn"
    },
    "extends": "google"
}
